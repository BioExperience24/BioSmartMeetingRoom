<div id="room-card-%room_id%" class="col-lg-6 col-md-6 col-sm-12 col-xs-12" onclick="onReserveRoom(this)">
    <div class="media">
        <div class="media-left media-middle">
            <a href="javascript:void(0);">
                <img class="media-object" src="%room_image%" width="100" height="100">
            </a>
        </div>
        <div class="media-body">
            <h4 class="media-heading">
                <a data-toggle="popover" data-placement="bottom" title="Popover title" tabindex="0" data-trigger="focus" data-content='' data-html="true">%room_name% </a>
            </h4>
            <div class="row clearfix">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <table class="table borderless table-borderless ">
                        <tr>
                            <td style="padding:2px !important; border:0px; ">
                                <i class="material-icons" style="" data-toggle="tooltip" data-placement="top" title="" data-original-title="Location">location_city</i>
                            </td>
                            <td style="padding:2px !important; border:0px; ">%room_building%</td>
                        </tr>
                        <tr>
                            <td style="padding:2px !important; border:0px; ">
                                <i class="material-icons" style="" data-toggle="tooltip" data-placement="top" title="" data-original-title="People/Attendees Capacity">people_outline</i>
                            </td>
                            <td style="padding:2px !important; border:0px; ">%room_capacity%</td>
                        </tr>
                    </table>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-xs-12">
                <button type="button" class="btn btn-success btn-block" aria-haspopup="true" aria-expanded="false">
                    Reserve Room <!-- <span class="caret"></span> -->
                </button>
            </div>
        </div>
    </div>  
</div>