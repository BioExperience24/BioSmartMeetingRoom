                <input asp-for="AccessControl.Type" class="form-control" />
                <span asp-validation-for="AccessControl.Type" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessControl.RoomControllerFalco" class="control-label"></label>
                <input asp-for="AccessControl.RoomControllerFalco" class="form-control" />
                <span asp-validation-for="AccessControl.RoomControllerFalco" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Create" class="btn btn-primary" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-page="Index">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
