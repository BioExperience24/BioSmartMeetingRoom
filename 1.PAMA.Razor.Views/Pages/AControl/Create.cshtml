@page
@model _1.PAMA.Razor.Views.Pages.AControl.CreateModel
@{
    ViewData["Title"] = "Create";
}

<h4>AccessControl</h4>
<hr />
<div class="row">
    <div class="col-md-4">
        <form method="post">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="AccessControl.Name" class="control-label"></label>
                <input asp-for="AccessControl.Name" class="form-control" />
                <span asp-validation-for="AccessControl.Name" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessControl.IpController" class="control-label"></label>
                <input asp-for="AccessControl.IpController" class="form-control" />
                <span asp-validation-for="AccessControl.IpController" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessControl.AccessId" class="control-label"></label>
                <input asp-for="AccessControl.AccessId" class="form-control" />
                <span asp-validation-for="AccessControl.AccessId" class="text-danger"></span>
            </div>
            <div class="form-group">
                <label asp-for="AccessControl.Channel" class="control-label"></label>
                <input asp-for="AccessControl.Channel" class="form-control" type="number" />
